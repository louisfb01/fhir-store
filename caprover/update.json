{
    "appName": "fhir-store",
    "instanceCount": 1,
    "notExposeAsWebApp": false,
    "containerHttpPort": "$CODA_FHIR_STORE_PORT",
    "forceSsl": false,
    "envVars": [
        {
            "key": "CODA_FHIR_STORE_PORT",
            "value": "$CODA_FHIR_STORE_PORT"
        },
        {
            "key": "CODA_FHIR_STORE_FHIR_VERSION",
            "value": "$CODA_FHIR_STORE_FHIR_VERSION"
        },
        {
            "key": "CODA_FHIR_STORE_LICENSE_ID",
            "value": "$CODA_FHIR_STORE_LICENSE_ID"
        },
        {
            "key": "CODA_FHIR_STORE_LICENSE_KEY",
            "value": "$CODA_FHIR_STORE_LICENSE_KEY"
        },
        {
            "key": "CODA_FHIR_STORE_CLIENT_ID",
            "value": "$CODA_FHIR_STORE_CLIENT_ID"
        },
        {
            "key": "CODA_FHIR_STORE_CLIENT_SECRET",
            "value": "$CODA_FHIR_STORE_CLIENT_SECRET"
        },
        {
            "key": "CODA_FHIR_STORE_DB_HOST",
            "value": "$CODA_FHIR_STORE_DB_HOST"
        },
        {
            "key": "CODA_FHIR_STORE_ADMIN_USERNAME",
            "value": "$CODA_FHIR_STORE_ADMIN_USERNAME"
        },
        {
            "key": "CODA_FHIR_STORE_ADMIN_PASSWORD",
            "value": "$CODA_FHIR_STORE_ADMIN_PASSWORD"
        },
        {
            "key": "CODA_FHIR_STORE_DB_USERNAME",
            "value": "$CODA_FHIR_STORE_DB_USERNAME"
        },
        {
            "key": "CODA_FHIR_STORE_DB_PASSWORD",
            "value": "$CODA_FHIR_STORE_DB_PASSWORD"
        },
        {
            "key": "CODA_FHIR_STORE_DB_NAME",
            "value": "$CODA_FHIR_STORE_DB_NAME"
        },
        {
            "key": "CODA_FHIR_STORE_DB_PORT",
            "value": "$CODA_FHIR_STORE_DB_PORT"
        }
    ]
}